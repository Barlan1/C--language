#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n=100;int count =0;
	
	int a[100]={0};
	for(int i=1;i<=100;i++){
		a[i]=i;
	}
	for(int i=1;i<=n;i++){
		int sum=0;
		if(a[i]%3==0){
			while(a[i]>0){
				sum+= a[i]%10;
				a[i]/=10;
			}
			if(sum%2==0){
				count++;
			}
		cout<<a[i];
	   }		
    }
	cout<<"\n"<< count;
	return count;
}
